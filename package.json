{
  "name": "semantic-sphere",
  "version": "1.0.0",
  "description": "Real-time 3D semantic concept mapping application with 3DConnexion SpaceMouse integration and OpenAI-powered dynamic knowledge graphs",
  "main": "dist/main/main.js",
  "homepage": "https://github.com/nicsaiart1/semantic-sphere",
  "author": {
    "name": "nicsaiart1",
    "url": "https://github.com/nicsaiart1"
  },
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:main\" \"npm run dev:renderer\"",
    "dev:main": "tsc -p src/main --watch",
    "dev:renderer": "vite",
    "build": "npm run build:main && npm run build:renderer",
    "build:main": "tsc -p src/main",
    "build:renderer": "vite build",
    "build:all": "npm run build && npm run dist",
    "build:windows": "npm run build && electron-builder --windows",
    "build:macos": "npm run build && electron-builder --macos",
    "build:linux": "npm run build && electron-builder --linux",
    "dist": "electron-builder",
    "start": "electron .",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.0",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/three": "^0.155.1",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "@vitejs/plugin-react": "^4.0.4",
    "concurrently": "^8.2.1",
    "electron": "^25.8.1",
    "electron-builder": "^24.6.4",
    "eslint": "^8.49.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-electron": "^0.14.1"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.14.8",
    "@react-spring/three": "^9.7.3",
    "@react-spring/web": "^9.7.3",
    "@react-three/drei": "^9.83.6",
    "@react-three/fiber": "^8.14.1",
    "axios": "^1.5.0",
    "d3": "^7.8.5",
    "electron-store": "^8.1.0",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.21",
    "openai": "^4.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "three": "^0.155.0"
  },
  "build": {
    "appId": "com.nicsaiart1.semantic-sphere",
    "productName": "SemanticSphere",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "assets/**/*",
      "package.json"
    ],
    "mac": {
      "category": "public.app-category.productivity",
      "target": [
        {
          "target": "dmg",
          "arch": ["x64", "arm64"]
        }
      ]
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": ["x64"]
        }
      ]
    },
    "linux": {
      "target": [
        {
          "target": "AppImage",
          "arch": ["x64"]
        }
      ]
    }
  }
}